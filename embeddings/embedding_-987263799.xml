            <gmd:title>
              <gco:CharacterString>Kaart van de bodemopbouw in Nederland 1:50.000 onder INSPIRE</gco:CharacterString>
            </gmd:title>
            <gmd:date xlink:type="simple">
              <gmd:CI_Date>
                <gmd:date>
                  <gco:Date>2014-12-18</gco:Date>
                </gmd:date>
                <gmd:dateType>
